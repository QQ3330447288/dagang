<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC
        "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
        "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">
<hibernate-mapping>
	<class name="entity.Complaint" table="Complaint">
		<id name="id" type="int">
			<generator class="native" />
		</id>
		<property name="complainant" type="java.lang.String" />
		<property name="complainObj" type="java.lang.String" />
		<property name="complainInfo" type="java.lang.String" />
		<property name="addtime" type="date" />
	</class>
</hibernate-mapping>